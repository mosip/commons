    return "014";
  } else if (itc == Resident && genderType == Others && ageCode == CHILD && isBioExPresent) {
    return "015";
  } else if (itc == Resident && genderType == Others && ageCode == ADULT && isBioExPresent) {
    return "016";
  }
  return "000";
}