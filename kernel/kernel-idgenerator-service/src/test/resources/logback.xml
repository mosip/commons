<configuration>
	<appender name="STDOUT"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d [%t] %p [%C{1}].%M.%L : %m%n</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

	<logger name="io.vertx.core.impl.BlockedThreadChecker"
		level="OFF" />
		
</configuration>